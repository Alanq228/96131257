    function b() {}
    for (var c = "assert,count,debug,dir,dirxml,error,exception,group,groupCollapsed,groupEnd,info,log,timeStamp,profile,profileEnd,time,timeEnd,trace,warn".split(","), d; d = c.pop();) {
        a[d] = a[d] || b
    }
})(function () {
    try {
        console.log();
        return window.console
    } catch (a) {
        return window.console = {}
    }
}());
